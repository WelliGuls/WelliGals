<%= form_for Meetup.new do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title%><br />

  <%= f.label :place %>:
  <%= f.text_field :place %><br />

  <%= f.label :date %>:
  <%= f.date_field :date %><br />


  <%= f.label :description %>:
  <%= f.text_area :description %><br />

  <%= f.label :capacity %>:
  <%= f.number_field :capacity %><br />

  <%= f.label :going %>:
  <%= f.number_field :going %><br />

  <%= f.submit "Create Meetup" %>

<%end%>

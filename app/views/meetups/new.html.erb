<%= form_for Meetup.new do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title%><br />

  <%= f.label :place %>:
  <%= f.text_field :place %><br />

  <%= f.label :date %>:
  <%= f.text_field :date %><br />


  <%= f.label :description %>:
  <%= f.text_field :description %><br />

  <%= f.submit "Create Meetup" %>

<%end%>
